<Window x:Class="ProjectsManager.LoginEnter"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="LoginEnter" Height="330" Width="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="93*"/>
            <ColumnDefinition Width="53*"/>
        </Grid.ColumnDefinitions>
        <ListBox Name="lbRegisteredUsers" HorizontalAlignment="Left" Height="163" Margin="23,37,0,0" VerticalAlignment="Top" Width="243" Grid.ColumnSpan="2"/>
        <Label Content="Enter with your's account:" HorizontalAlignment="Left" Margin="23,6,0,0" VerticalAlignment="Top" Width="243" Grid.ColumnSpan="2"/>
        <Button Name="btnLogin" Content="Enter" HorizontalAlignment="Left" Height="32" Margin="23,258,0,0" VerticalAlignment="Top" Width="123" />
        <PasswordBox Name="pbPasswordField" HorizontalAlignment="Left" Margin="110,214,0,0" VerticalAlignment="Top" Grid.ColumnSpan="2" Width="156" Height="27"/>
        <Label Content="Password:" HorizontalAlignment="Left" Margin="23,215,0,0" VerticalAlignment="Top" Width="82"/>

    </Grid>
</Window>
